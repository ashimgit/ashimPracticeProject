package pop;

public class TowerInfoBean 
{
	private String tower_id;
	private String tower_site;
	private String tower_address;
	private String tower_type;
	private String district_name;
	private String state_name;
	private String clustermanager_name;
	private String serviceprovider_name;
	
	TowerInfoBean(String tower_id,String tower_site,String tower_address,String tower_type,String district_name,String state_name)
	
	{
		
		this.tower_id=tower_id;
		this.tower_site=tower_site;
		this.tower_address=tower_address;
		this.tower_type=tower_type;
		this.district_name=district_name;
		this.state_name=state_name;
		//this.clustermanager_name=clustermanager_name;
		//this.serviceprovider_name=serviceprovider_name;
		
		
	}
	
	public String toString()
	{
		return "<tr><td>"+tower_id+"</td><td>"+tower_site+"</td><td>"+tower_address+"</td><td>"+tower_type+"</td><td>"+district_name+"</td><td>"+state_name+"</td></tr>";
	}
}
